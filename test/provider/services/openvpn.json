{
  "openvpn": {
    "ports": ["80", "443", "53", "1194"],
    "filter_dns": false,
    "nat": true,
    "ca_crt": "= file 'ca/ca.crt'",
    "ca_key": "= file 'ca/ca.key'",
    "server_crt": "= file domain.name + '.crt'",
    "server_key": "= file domain.name + '.key'"
  },
  "service_type": "user_service"
}
