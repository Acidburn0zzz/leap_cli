#
#  "server_crt": "= file :node_x509_cert",
#  "server_key": "= file :node_x509_key"
#
{
  "service_type": "user_service",
  "x509": {
    "use": true
  },
  "openvpn": {
    "ports": ["80", "443", "53", "1194"],
    "filter_dns": false,
    "nat": true,
    "ca_crt": "= file :ca_cert",
    "ca_key": "= file :ca_key",
    "dh": "= file :dh_params"
  }
}
